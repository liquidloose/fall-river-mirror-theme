			],
			"fontSizes": [
				{
					"fluid": true,
					"name": "Small",
					"size": "1 rem",
					"slug": "small"
				},
				{
					"fluid": true,
					"name": "Medium",
					"size": "1.25rem",
					"slug": "medium"
				},
				{
					"fluid": true,
					"name": "Large",
					"size": "1.5rem",
					"slug": "large"
				},
				{
					"fluid": true,
					"name": "Extra Large",
					"size": "2rem",
					"slug": "x-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/column": {
				"border": {
					"radius": "0px"
				}
			},
			"core/columns": {
				"border": {
					"radius": "0px"
				}
			},
			"core/image": {
				"border": {
					"bottom": {
						"style": "none",
						"width": "0px"
					},
					"color": null,
					"left": {
						"style": "none",
						"width": "0px"
					},
					"radius": "0px",
					"right": {
						"style": "none",
						"width": "0px"
					},
					"style": null,
					"top": {
						"style": "none",
						"width": "0px"
					},
					"width": null
				},
				"shadow": "6px 6px 9px rgba(0, 0, 0, 0.2)"
			},
			"core/paragraph": {
				"spacing": {
					"padding": {
						"left": "0",
						"right": "0"
					}
				}
			},
			"core/post-featured-image": {
				"border": {
					"radius": "0px"
				},
				"shadow": "6px 6px 9px rgba(0, 0, 0, 0.2)"
			}
		},
		"elements": {
			"caption": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto-mono)"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto-mono)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--body-text)"
				}
			}
		},
		"spacing": {
			"blockGap": "0px"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--roboto)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	]
}